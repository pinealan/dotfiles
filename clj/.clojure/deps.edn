{:deps
 {org.clojure/clojure {:mvn/version "1.10.1"}}

 :aliases
 {:cljfmt {:extra-deps {cljfmt {:mvn/version "0.6.4"}}
           :main-opts ["-m" "cljfmt.main"]}

  :repl   {:extra-deps {nrepl {:mvn/version "0.7.0"}}
           :main-opts ["-m" "nrepl.cmdline" "--connect" "--port"]}

  :iced   {:extra-deps {nrepl {:mvn/version "0.7.0"}
                        iced-nrepl {:mvn/version "1.0.1"}
                        refactor-nrepl {:mvn/version "2.5.0"}
                        cider/cider-nrepl {:mvn/version "0.24.0"}
                        iced-repl {:local/root "/Users/alan/.vim/plugged/vim-iced"
                                   :deps/manifest :deps}
                        needleware {:local/root "/Volumes/Data/alan/lab/needleware"
