           :main-opts ["-m" "nrepl.cmdline"
                       "--middleware"
                       "[cider.nrepl/wrap-classpath,cider.nrepl/wrap-clojuredocs,cider.nrepl/wrap-complete,cider.nrepl/wrap-debug,cider.nrepl/wrap-format,cider.nrepl/wrap-info,cider.nrepl/wrap-macroexpand,cider.nrepl/wrap-ns,cider.nrepl/wrap-out,cider.nrepl/wrap-spec,cider.nrepl/wrap-test,cider.nrepl/wrap-trace,cider.nrepl/wrap-undef,cider.nrepl/wrap-xref,refactor-nrepl.middleware/wrap-refactor,iced.nrepl/wrap-iced,needleware.print/print-eval]"]}

  :nrepld {:extra-deps {nrepl {:mvn/version "0.7.0"}
                        cider/cider-nrepl {:mvn/version "0.22.4"}}
           :main-opts ["-m" "nrepl.cmdline"
                       "--middleware" "[cider.nrepl/cider-middleware]"]}}}
