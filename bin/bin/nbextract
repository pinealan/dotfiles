#!/usr/bin/env python3

import json
import sys
import re

notebook = json.load(sys.stdin)

for cell in notebook['cells']:
    if cell['source'] == []:
        continue

    lines = []
    for line in cell['source']:
        if cell['cell_type'] == 'code':
            lines.append(re.sub(r'^%', '# %', line))
        elif cell['cell_type'] == 'markdown':
            lines.append('# ' + line)
        else:
            lines.append(line)

    print(''.join(lines))
    print()
    print('#' + '-' * 40)
